pipeline {
  environment {
    registry = "testdocker514/devmbb"
    registryCredential = 'dockerhub'
  }
  agent any
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/KiranKumar514/chef-docker.git'
      }
    }
    stage('Building image') {
      steps{
        script {
          docker build -t "chef" .
             }
      }
    }
  }
}
